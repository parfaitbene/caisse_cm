        <record model="ir.sequence" id="customer_account_seq">
			<field name="name">Customer account sequence</field>
			<field name="code">customer.account.seq</field>
			<field name="prefix">4111</field>
			<field name="padding">5</field>
		</record>
				
		<record model="ir.sequence" id="supplier_account_seq">
			<field name="name">Supplier account sequence</field>
			<field name="code">supplier.account.seq</field>
			<field name="prefix">4011</field>
			<field name="padding">5</field>
		</record>
		
